<%= form_for @song, url: {action: 'create'} do |f| %>
<label>Song Name:</label> <%= f.text_field :name %><br/>
<label>Artist:</label><br/>

<% @artists.each do |artist| %>
  <%= f.radio_button :'artist_id', artist.id %> <%= artist.name %><br/>
<% end %>
<br/>

<% @genres.each do |genre| %>
  <%= f.radio_button :'genre_id', genre.id %> <%= genre.name %><br/>
<% end %>

<%= f.submit "Create Song" %>
<% end %>
