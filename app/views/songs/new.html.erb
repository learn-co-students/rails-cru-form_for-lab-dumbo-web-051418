<h2>New Song!</h2>
<%= form_for @song do |f| %>
<%= f.label :name, "Song: " %>
<%= f.text_field :name, placeholder: "Song Title"%><br>
<%= f.label :artist, "Artist: " %>
<%= f.text_field :artist, placeholder: "Artist"%><br>
<%= f.label :genre, "Genre: " %>
<%= f.text_field :genre, placeholder: "Genre"%><br>
<%= f.submit "Create Song" %>
<% end %>
