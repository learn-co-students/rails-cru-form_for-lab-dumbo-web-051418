<h1>Song Edit Form</h1>
<%= form_for @song, method: "put" do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :artist_id %>
<%= f.text_field :artist_id %>
<!-- <%= f.select :artist_id, Artist.all.collect { |p| [ p.name, p.id ] }, include_blank: true %> -->
<%= f.label :genre_id %>
<%= f.text_field :genre_id %>
<!-- <%= f.select :genre_id, Genre.all.collect { |p| [ p.name, p.id ] }, include_blank: true %> -->
  <%= f.submit "Update Song" %>
<% end %>
